---


import listaimgheader from '../../config/listaimgheader.json'
import ImgHeader from "./ImgHeader.astro";
import {Shuffle} from '../../utils/Shuffle.js'

Shuffle(listaimgheader);


---
<ul class="flex flex-wrap m-auto w-10/12 justify-center">
  {
    listaimgheader.map(({icon,nameicon,customstyles})=>( 
      
      <ImgHeader icon={icon} nameicon={nameicon} customstyles={customstyles} ></ImgHeader>
  
  
    ))
    
  }
</ul>












